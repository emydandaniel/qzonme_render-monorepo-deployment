  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:win": "set NODE_ENV=development && tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --packages=external --external:./vite --external:./vite.js --external:vite --define:process.env.NODE_ENV='\"production\"'",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --bundle --format=esm --outdir=dist --packages=external --external:./vite --external:./vite.js --external:vite --define:process.env.NODE_ENV='\"production\"'",
    "start": "NODE_ENV=production node dist/index.js",
    "start:server": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:setup": "echo 'Database setup handled by Render'",
    "test:health": "curl http://localhost:5000/health || echo 'Server not running'",
    "docker:build": "docker build -t qzonme .",
    "docker:run": "docker run -p 10000:10000 qzonme",
    "deploy:check": "npm run build && npm run check"
  },
